+                               gd->env_valid = ENV_VALID;
+                               break;
+                       case SD1_BOOT :
+                               gd->env_addr = (ulong)&default_environment_msd1[0];
+                               gd->env_valid = ENV_VALID;
+                               break;
+                       case SD2_BOOT :
+                               gd->env_addr = (ulong)&default_environment_msd2[0];
+                               gd->env_valid = ENV_VALID;
+                               break;
+                       default:
+                               gd->env_addr = (ulong)&default_environment[0];
+                               gd->env_valid = ENV_VALID;
+                               break;
+               }
+#else
 		gd->env_addr = (ulong)&default_environment[0];
 		gd->env_valid = ENV_VALID;
+#endif
 
 		return 0;
 	}
