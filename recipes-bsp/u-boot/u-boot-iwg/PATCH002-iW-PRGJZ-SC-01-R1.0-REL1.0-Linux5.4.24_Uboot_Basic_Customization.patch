+	   revision |= gpio_get_value(board_config_pads[i]) << i;
+     }
+
+     pcb_rev = ((revision) & 0x03) +1;
+     bom_rev = (((revision) & 0x78)>>3);
+
+     printf ("Board Info:\n");
+     printf ("\tBSP Version               : %s\n", BSP_VERSION);
+     printf ("\tSOM Version               : iW-PRGJZ-AP-01-R%x.%x\n",pcb_rev,bom_rev);
