+static int hyn_boot_update_fw(struct i2c_client * client)
+{
+	unsigned char *ptr_fw;
+	int ret;
+	ptr_fw = pcst3xx_update_firmware;
+	
+	ret = cst3xx_boot_update_fw(client, ptr_fw);
+    return ret;
+	
+}
