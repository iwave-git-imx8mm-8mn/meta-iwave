+static struct file *cst3xx_open_fw_file(char *path, mm_segment_t * old_fs_p)
+{
+	struct file * filp;
+	int ret;
+	
+	*old_fs_p = get_fs();
+	set_fs(KERNEL_DS);
+	filp = filp_open(path, O_RDONLY, 0);
+	if (IS_ERR(filp)) 
+	{
+        ret = PTR_ERR(filp);
+        return NULL;
+    }
+    filp->f_op->llseek(filp, 0, 0);
+	
+    return filp;
+}
