+static int cst3xx_check_checksum(struct i2c_client * client)
+{
+	int ret;
+	int i;
+	unsigned int  checksum;
+	unsigned int  bin_checksum;
+	unsigned char buf[4];
+	const unsigned char *pData;
+
+	for(i=0; i<5; i++)
+	{
+		buf[0] = 0xA0;
+		buf[1] = 0x00;
+		ret = cst3xx_i2c_read_register(client, buf, 1);
+		if(ret < 0)
+		{
+			mdelay(2);
+			continue;
+		}
+
+		if(buf[0]!=0)
+			break;
+		else
+		mdelay(2);
+	}
+    mdelay(2);
+
+
+    if(buf[0]==0x01)
