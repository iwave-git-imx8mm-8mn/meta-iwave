+static int gtp_fb_notifier_callback(struct notifier_block *noti, unsigned long event, void *data)
+{
+	struct fb_event *ev_data = data;
+	int *blank;
+	struct cst3xx_ts_data *ts=NULL;
+	ts=i2c_get_clientdata(g_i2c_client);
+	if(!ts)
