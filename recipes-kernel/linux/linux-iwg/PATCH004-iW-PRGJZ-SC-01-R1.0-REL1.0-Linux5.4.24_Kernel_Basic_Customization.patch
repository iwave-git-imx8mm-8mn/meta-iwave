+	buf[0] = 0xA0;
+	buf[1] = 0x06;
+	buf[2] = 0xEE;
+	ret = cst3xx_i2c_write(client, buf, 3);
+	if (ret < 0)
+		return -1;
+	
+	mdelay(10);	//wait for restart
+
+	
+	return 0;
