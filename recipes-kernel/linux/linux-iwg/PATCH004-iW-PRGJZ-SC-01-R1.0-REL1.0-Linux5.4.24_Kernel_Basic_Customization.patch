diff -Naur A/arch/arm64/boot/dts/freescale/imx8mn-iwg37d_2gb.dts B/arch/arm64/boot/dts/freescale/imx8mn-iwg37d_2gb.dts
--- A/arch/arm64/boot/dts/freescale/imx8mn-iwg37d_2gb.dts	1970-01-01 05:30:00.000000000 +0530
+++ B/arch/arm64/boot/dts/freescale/imx8mn-iwg37d_2gb.dts	2021-01-21 15:21:45.942008566 +0530
@@ -0,0 +1,405 @@
+/*
