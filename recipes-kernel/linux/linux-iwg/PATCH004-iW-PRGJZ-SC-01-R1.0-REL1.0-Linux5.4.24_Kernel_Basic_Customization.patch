+    unsigned char cnt_up, cnt_down;
+	int   i, ret, idx; 
+	int cnt, i2c_len;
+
+	struct cst3xx_ts_data *ts=NULL;
+#ifdef TRANSACTION_LENGTH_LIMITED
+	int  len_1, len_2;
+#endif
+	ts=container_of(work,struct cst3xx_ts_data,work);	
+
+	GTP_DEBUG_FUNC(); 
+
+#ifdef HYN_GESTURE
