+/*******************************************************
+Function:
+    test i2c communication
+Input:
+    client: i2c client
+Output:
+
+    success: big than 0
+    fail:	negative
+*******************************************************/
+static int cst3xx_i2c_test(struct i2c_client *client)
+{
+	int retry = 0;
+	int ret;
+	unsigned char buf[4];
+
+	buf[0] = 0xD1;
+	buf[1] = 0x06;
+	while (retry++ < 5) {
+		ret = cst3xx_i2c_write(client, buf, 2);
+		if (ret > 0)
+			return ret;
+		
+		mdelay(2);		
+	}
+
+    if(retry==5) printk("linc cst3xx hyn I2C TEST error.ret:%d;\n", ret);
+	
+	return ret;
+}
