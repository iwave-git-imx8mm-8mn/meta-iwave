+void cst3xx_irq_disable(struct cst3xx_ts_data *ts)
+{
+	unsigned long irqflags;
+	
+	GTP_DEBUG_FUNC(); 
+	spin_lock_irqsave(&ts->irq_lock, irqflags);
+	if (!irq_is_disable) {
+		irq_is_disable = 1;
+		disable_irq_nosync(ts->client->irq);
+	}
+	spin_unlock_irqrestore(&ts->irq_lock, irqflags);
+}
