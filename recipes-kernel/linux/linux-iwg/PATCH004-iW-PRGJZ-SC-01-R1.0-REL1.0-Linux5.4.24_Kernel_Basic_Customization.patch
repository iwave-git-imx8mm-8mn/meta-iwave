+
+/dts-v1/;
+
+#include "imx8mn-iwg37m.dts"
+
+/ {
+	model = "iW-RainboW-G37M-i.MX8MN-SODIMM-1GB";
+	compatible = "fsl,imx8mn-iwg37m", "fsl,imx8mn";
+
+	/* external oscillator node */
+	osc: oscillator {
+		compatible = "fixed-clock";
+		#clock-cells = <1>;
+		clock-frequency  = <32768>;
+		clock-output-names = "osc";
+	};
+
+	audioclk: ext26Mz { /* SAI5: Clock Configuration */
+		compatible = "fixed-clock";
+		clock-frequency = <26000000>;
+		#clock-cells = <0>;
+	};
+
+	regulators {
+		compatible = "simple-bus";
+		#address-cells = <1>;
+		#size-cells = <0>;
+
+		reg_3p3v: 3p3v { /* Fixed Regulator: common 3.3V Fixed regulator */
+			compatible = "regulator-fixed";
+			regulator-name = "3P3V";
+			regulator-min-microvolt = <3300000>;
+			regulator-max-microvolt = <3300000>;
+			regulator-always-on;
+		};
+	};
