+&rpmsg{
+        /*
+         * 64K for one rpmsg instance:
+         * --0xb8000000~0xb800ffff: pingpong
+         */
+        vdev-nums = <1>;
+        reg = <0x0 0xb8000000 0x0 0x10000>;
+        memory-region = <&vdevbuffer>;
+        status = "okay";
