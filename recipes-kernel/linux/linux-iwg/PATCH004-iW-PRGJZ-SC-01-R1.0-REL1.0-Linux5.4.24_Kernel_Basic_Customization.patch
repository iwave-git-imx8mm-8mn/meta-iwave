+static void cst3xx_enter_sleep(struct i2c_client *client)
+{
+	int ret;
+	int retry = 0;
+	unsigned char buf[2];
+
+    buf[0] = 0xD1;
+	buf[1] = 0x05;
+	while (retry++ < 5) {
+		ret = cst3xx_i2c_write(client, buf, 2);
+		if (ret > 0)
+			return;
+		mdelay(2);
+	}
+	
+	return;
+}
